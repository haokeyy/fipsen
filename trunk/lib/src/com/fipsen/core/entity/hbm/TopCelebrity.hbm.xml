<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fipsen.core.entity.TopCelebrity" table="t_topcelebrity">
		<id 
			name="oid" column="oid">			
		</id> 	
 	
 		<property name="firstName" 	column="firstname" 		not-null="false"	lazy="false"/>
		<property name="lastName" 	column="lastname" 		not-null="false"	lazy="false"/>
		<property name="middleName" column="middlename"		not-null="false"	lazy="false"/>
		<property name="position" 	column="position"		not-null="false"	lazy="false"/>
	</class>
</hibernate-mapping>